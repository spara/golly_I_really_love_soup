{
	"manifest_version": 2,
	"name": "Golly, I love soup",
	"version": "1.0",
	"description": "Replaces the text 'Trump' with 'Golly, I love soup'.",
	"icons": {"256":"icon.png"},
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
